{
  "name": "deploy-aws-beanstalk",
  "author": "Amrane Ait Zeouay",
  "version": "1.0.2",
  "description": "GitHub Action to deploy a project to AWS Elastic Beanstalk.",
  "license": "Apache-2.0",
  "main": "deploy-beanstalk.js",
  "scripts": {
    "start": "nodemon",
    "build": "rimraf ./build && tsc",
    "build:prod": "ncc build src/deploy-beanstalk.ts -m -o build",
    "lint": "eslint . --ext .ts",
    "prettier": "pretty-quick --staged",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "deploy-beanstalk": "./deploy-beanstalk.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dealermade/aws-beanstalk-deploy.git"
  },
  "keywords": [
    "S3",
    "AWS",
    "deploy",
    "actions",
    "variables",
    "github actions",
    "elastic beanstalk"
  ],
  "bugs": {
    "url": "https://github.com/Dealermade/aws-beanstalk-deploy/issues"
  },
  "homepage": "https://github.com/Dealermade/aws-beanstalk-deploy",
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run lint"
    }
  },
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@types/node-fetch": "^2.6.2",
    "aws-sdk": "^2.1221.0",
    "node-fetch": "^3.2.10",
    "stream": "0.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^8.23.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
